{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "feel-tauri-mobile",
  "version": "0.0.1",
  "identifier": "com.tauri.mobile",
  "build": {
    "devUrl": "http://localhost:11420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "feel-tauri-mobile",
        "width": 800,
        "height": 600,
        "fullscreen": false,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  },
  "plugins": {
    "updater": {
      "active": false
    }
  }
}